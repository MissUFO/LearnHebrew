<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="com.rockislandbooks.hebrewcompanion" version="1.0.20" xmlns="http://www.w3.org/ns/widgets" defaultlocale="en-US" android-versionCode="">
  <name>3D Hebrew Companion</name>
