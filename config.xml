  <preference name="ShowTitle" value="True" />
  <preference name="InAppBrowserStorageEnabled" value="True" />
  <preference name="SuppressesIncrementalRendering" value="True" />
  <vs:plugin name="org.apache.cordova.splashscreen" version="1.0.0" />
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows" version="1.0.0.1">
      <vs:certFile>CordovaApp.pfx</vs:certFile>
      <vs:packageOutputPath>C:\Work\Elance\LearnHebrew\DEP\</vs:packageOutputPath>
      <vs:name>3D Hebrew Companion</vs:name>
